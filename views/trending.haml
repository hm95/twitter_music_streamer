!!!
%html
  %head
    %title mus.iq

    %script{src:"//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"}
    %script{src: "http://code.highcharts.com/highcharts.js"}
    %script{src: "scripts/trending.js", type: "text/javascript"}
    %script{src: "scripts/player.js", type: "text/javascript"}
    %link{href: "http://fonts.googleapis.com/css?family=Lato:400,700|Convergence|Oxygen:400,700|Imprima|Oleo+Script+Swash+Caps", rel: "stylesheet", type: "text/css"}
    %link{href: "css/home.css", rel: "stylesheet", type: "text/css"}

  %body
    #titlebar
      #title
        %a{href: "/"} mus.iq
      #tabs
        %a#trending.selected
          Trending
        %a#popular{href: "/"}
          Popular

    .container2
      / .vid#template{style: "display:none;"}
      /   .vidDetails
      /     .rank
      /     .vidTitle
      /     .vidArtist

      / Hidden overlay div
      #overlay
        #videoContainer
          :plain
            <iframe width="550px" height="400" src="" frameborder="0" allowfullscreen></iframe>
        #analyticsContainer
          #tweetTrend{class: "graph", style: "width: 300px; display: inline-block; position: relative; height: 200px;"}
          #language{class: "graph", style: "width: 300px; display: inline-block; position: relative; height: 200px;"}
          #geo{class: "graph", style: "width: 300px; display: inline-block; position: relative; height: 200px;"}
